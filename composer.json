{
    "name": "notfloran/mjml-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for MJML",
    "keywords": ["mjml", "email"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NotFloran\\MjmlBundle\\": "src"
        }
    },
    "authors": [
        {
            "name": "Floran Brutel",
            "email": "me@floran.fr"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "symfony/process": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/dependency-injection": "^3.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "suggest": {
        "swiftmailer/swiftmailer": "Plugin to integrate MJML in SwiftMailer"
    }
}
